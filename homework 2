#include <stdio.h>

	int main(){
		 
		int a = 0;
		int b = 0;
			
			printf ("Enter the first number:" );
			scanf ("%d",&a);
			printf ("Enter the second number:");
			scanf ("%d",&b);

			if (a>b){
				printf("%d",a);
			}else if (a==b){
				printf("a=b");
			}else {
			       printf("%d",b);}
		return 0;
	}	

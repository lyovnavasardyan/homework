#include <stdio.h>

    int main(){
        char letter;
        printf ("Enter a letter:");
        scanf("%c",&letter);
        
        if (letter==65||letter==69||letter==73||letter==79||letter==85||letter==89||letter==97||letter==101||letter==105||letter==111||letter==117||letter==121){
            printf ("Dzaynavor ");
        }else{
            printf("Baxadzayn ");
        }
        return 0;
    }

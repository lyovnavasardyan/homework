#include <stdio.h>
  int main (){
    int a = 0;
    int b = 0;
    int c = 0;
    
        printf ("Enter the first number : ");
        scanf ("%d",&a);
        printf ("Enter the second number : ");
        scanf ("%d",&b);
        printf ("Enter the third number : ");
        scanf ("%d",&c);
        
        if ((a==b || a==c || b==c)&&(a>b || a>c)){
            printf ("%d",a);
        }
        else if ((b==a || b==c || a==c)&&(b>a || b>c)){
            printf ("%d",b);
        }
        else if ((c==a || c==b || b==a)&&(c>a || c>b)){
            printf ("%d",c);
        }
          else {
              printf("Try again:");
        }
        return 0;
  }
